          "maxItems": 10,
          "description": "Filter by relationship types"
        },
        "time_range": {
          "type": "object",
          "properties": {
            "start": {
              "type": "string",
              "format": "date-time"
            },
            "end": {
              "type": "string",
              "format": "date-time"
            }
          },
          "additionalProperties": false,
          "description": "Time range filter"
        },
        "max_generation": {
          "type": "integer",
          "minimum": 0,
          "maximum": 1000,
          "description": "Maximum generation to show"
        }
      },
      "additionalProperties": false
