        "view_mode": {
          "type": "string",
          "enum": ["full", "ancestors", "descendants", "blame", "impact"],
          "default": "full",
          "description": "Current view mode"
        },
        "layout_mode": {
          "type": "string",
          "enum": ["top_down", "timeline", "radial", "compact"],
          "default": "top_down",
